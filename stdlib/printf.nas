[FORMAT "WCOFF"]
[INSTRSET "i486p"]
[OPTIMIZE 1]
[OPTION 1]
[BITS 32]
	EXTERN	_vsprintf
	EXTERN	_api_putstr0
[FILE "printf.c"]
[SECTION .text]
	GLOBAL	_printf
_printf:
	PUSH	EBP
	MOV	EBP,ESP
	PUSH	ESI
	PUSH	EBX
	SUB	ESP,1008
	LEA	EAX,DWORD [12+EBP]
	LEA	EBX,DWORD [-1016+EBP]
	PUSH	EAX
	PUSH	DWORD [8+EBP]
	PUSH	EBX
	CALL	_vsprintf
	PUSH	EBX
	MOV	ESI,EAX
	CALL	_api_putstr0
	LEA	ESP,DWORD [-8+EBP]
	MOV	EAX,ESI
	POP	EBX
	POP	ESI
	POP	EBP
	RET
